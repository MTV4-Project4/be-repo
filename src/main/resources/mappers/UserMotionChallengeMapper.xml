<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.walkers.sportslight.userMotionChallenge.query.repository.UserMotionChallengeQueryRepository">

    <select id="findSimilarityById" resultType="_double" parameterType="_long">
        SELECT
                SIMILARITY
          FROM
                USER_MOTION_CHALLENGE
         WHERE
                USER_MOTION_ID = #{userMotionChallengeId}
    </select>



</mapper>